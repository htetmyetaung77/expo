{
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.workingDirectories": [
    {
      "pattern": "*"
    },
  ],
  "workbench.editor.editorActionsLocation": "default",
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.alwaysShowEditorActions": true,
  "workbench.editor.autoLockGroups": {
    "default": true,
    "workbench.editorinputs.sidebysideEditorInput": true
  },
  "workbench.editor.defaultBinaryEditor": "jsProfileVisualizer.heapsnapshot.table",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.limit.excludeDirty": true,
  "workbench.editor.navigationScope": "editor",
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.preferHistoryBasedLanguageDetection": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.scrollToSwitchTabs": false,
  "workbench.editor.sharedViewState": true,
  "workbench.editor.wrapTabs": true,
  "editor.mouseWheelZoom": true,
  "editor.formatOnPaste": true,
  "editor.aiStats.enabled": true,
  "editor.allowVariableFontsInAccessibilityMode": true,
  "editor.autoIndentOnPaste": true,
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnSave": true,
  "codium.codeCompletion.enable": true,
  "firefox.keepProfileChanges": true,
  "codium.enterprise.onPremService.enable": true,
  "github.copilot.chat.scopeSelection": true,
  "github.copilot.chat.terminalChatLocation": "quickChat",
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "github.copilot.chat.edits.temporalContext.enabled": true,
  "github.copilot.chat.enableRetryAfterFilteredResponse": true,
  "github.copilot.chat.generateTests.codeLens": true,
  "github.copilot.chat.languageContext.fix.typescript.enabled": true,
  "github.copilot.chat.languageContext.inline.typescript.enabled": true,
  "github.copilot.chat.languageContext.typescript.enabled": true,
  "github.copilot.chat.newWorkspace.useContext7": true,
  "github.copilot.chat.notebook.followCellExecution.enabled": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.fullReload": true,
  "liveServer.settings.NoBrowser": true,
  "liveServer.settings.useLocalIp": true,
  "liveServer.settings.useWebExt": true,
  "liveServer.settings.CustomBrowser": "microsoft-edge"
}